{
  "name": "@tangled3/react",
  "private": false,
  "version": "1.3.0",
  "type": "module",
  "license": "MIT",
  "main": "./src/index.ts",
  "types": "./src/index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "scripts": {
    "watch": "tsc -w -p ./tsconfig.json",
    "build": "pnpm clean && pnpm build:esm && pnpm build:postcompile && pnpm build:clean",
    "build:esm": "tsc --build --verbose",
    "build:postcompile": "node ../../scripts/postcompile.js && cpy 'src/**/*' dist",
    "build:clean": "rm -rf tsconfig.tsbuildinfo ./dist/tsconfig.tsbuildinfo ./dist/tsconfig.cjs.tsbuildinfo",
    "clean": "pnpm build:clean && rm -rf dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepublishOnly": "pnpm build"
  },
  "author": "Jayesh Bhole <jayeshbhole123@gmail.com>",
  "contributors": [
    "Ankit Singh <ankit.singh.zm@gmail.com>"
  ],
  "dependencies": {
    "@nightlylabs/wallet-selector-polkadot": "^0.2.6",
    "@safe-global/safe-apps-sdk": "^9.0.0",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/web3.js": "^1.91.2",
    "@tangled3/solana-react": "workspace:*",
    "@tanstack/react-query": "^5.44.0",
    "@tronweb3/tronwallet-abstract-adapter": "^1.1.6",
    "@tronweb3/tronwallet-adapters": "^1.2.1",
    "@wagmi/core": "^2.11.0",
    "detect-browser": "^5.3.0",
    "immer": "^10.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "^2.13.8",
    "wagmi": "^2.9.11",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.44.0",
    "@wagmi/core": "^2.11.0",
    "react": ">=18",
    "react-dom": ">=18",
    "viem": "^2.13.8",
    "wagmi": "^2.9.11"
  }
}
